{
  "name": "nuxt-windicss",
  "version": "2.2.2",
  "description": "Windi CSS for Nuxt.js",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "windicss",
    "tailwindcss"
  ],
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "main": "./index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/windicss/nuxt-windicss",
  "bugs": "https://github.com/windicss/nuxt-windicss/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/windicss/nuxt-windicss"
  },
  "license": "MIT",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com"
  },
  "files": [
    "dist",
    "index.cjs"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@windicss/plugin-utils": "^1.6.1",
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "h3": "^0.3.8",
    "listhen": "^0.2.4",
    "pathe": "^0.2.0",
    "read-cache": "^1.0.0",
    "sirv": "^1.0.19",
    "vite-plugin-windicss": "^1.6.1",
    "windicss": "^3.4.2",
    "windicss-analysis": "^0.3.5",
    "windicss-webpack-plugin": "^1.6.2"
  },
  "devDependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxt/schema": "npm:@nuxt/schema-edge@latest",
    "@nuxt/types": "^2.15.8",
    "release-it": "^14.11.8"
  }
}
